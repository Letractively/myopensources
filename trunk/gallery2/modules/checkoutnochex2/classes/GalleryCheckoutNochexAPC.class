 */
GalleryCoreApi::requireOnce('modules/core/classes/GalleryChildEntity.class');

/**
 * A GalleryChildEntity for CheckoutNochex transactions.
 *
 * @g2 <class-name>GalleryCheckoutNochexAPC</class-name>
 * @g2 <parent-class-name>GalleryChildEntity</parent-class-name>
 * @g2 <schema>
 * @g2   <schema-major>1</schema-major>
 * @g2   <schema-minor>0</schema-minor>
 * @g2 </schema>
 * @g2 <requires-id/>
 *
 * @package CheckoutNochex
 * @subpackage Classes
 */
class GalleryCheckoutNochexAPC extends GalleryChildEntity {

    /*
     * ****************************************
     *                 Members
     * ****************************************
     */

     /**
     * To_email
     *
     * @g2 <member>
     * @g2   <member-name>ncToEmail</member-name>
     * @g2   <member-type>STRING</member-type>
     * @g2   <member-size>MEDIUM</member-size>
     * @g2 </member>
     *
     * @var $ncToEmail
     * @access public
     */
    var $ncToEmail;
    
    /**
