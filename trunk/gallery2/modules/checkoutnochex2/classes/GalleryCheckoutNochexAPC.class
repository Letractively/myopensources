    }
    
    function getNcFromEmail() {
        return $this->ncFromEmail;
    }

    function setNcFromEmail($ncFromEmail) {
        $this->ncFromEmail = $ncFromEmail;
    }

    function getNcTransactionId() {
        return $this->ncTransactionId;
    }

