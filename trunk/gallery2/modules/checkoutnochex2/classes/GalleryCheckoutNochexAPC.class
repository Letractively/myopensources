        return $this->ncStatus;
    }

    function setNcStatus($ncStatus) {
        $this->ncStatus = $ncStatus;
    }

    function getNotes() {
        return $this->notes;
    }

    function setNotes($notes) {
        $this->notes = $notes;
    }
